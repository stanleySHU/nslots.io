<autoSpin id="autoSpinOptions" max="99999999999999" fontSize="30" textAlign="right" textIndent="30px" color="#FFFFFF" >
	<layer id="keyboard" visible="false">
		<for from="0" to="10" step="1">
			<content>      
            	<buttonSlide id="{%n%}" command="onKeyboard" x="{%x%}" y="{%y%}" >
					<layer id="up">
						<label value="{%n%}" align="center" fontFamily="Din, Arial" fontSize="30" fill="0xFFFFFF" pivot="0,50%"/>
						<circle radius="29" color="0x333333" />
					</layer>
					<layer id="down">
						<label value="{%n%}" align="center" fontFamily="Din, Arial" fontSize="30" fill="0x333333" pivot="0,50%"/>
						<circle radius="29" color="0xF3DE3F" />
					</layer>
					<layer id="disable">
						<label value="{%n%}" align="center" fontFamily="Din, Arial" fontSize="30" fill="0xFFFFFF" pivot="0,50%"/>
						<circle radius="29" color="0x333333" />
					</layer>
				</buttonSlide>
			</content>			
			<arguments>
				<argument id="0" n="1" x="40" y="40"/>
				<argument id="1" n="2" x="112" y="40"/>
				<argument id="2" n="3" x="184" y="40"/>
				<argument id="3" n="4" x="256" y="40"/>
				<argument id="4" n="5" x="328" y="40"/>
				<argument id="5" n="6" x="40" y="112"/>
				<argument id="6" n="7" x="112" y="112"/>
				<argument id="7" n="8" x="184" y="112"/>
				<argument id="8" n="9" x="256" y="112"/>
				<argument id="9" n="0" x="328" y="112"/>
				<argument id="10" n="." x="400" y="112"/>
			</arguments>
		</for>
		<buttonSlide id="X" command="onKeyboard" x="400" y="40" >
			<layer id="up">
				<img res="@atlas/ui/cancel.png" pivot="50%,50%" />
				<circle radius="29" color="0x333333" />
			</layer>
			<layer id="down">
				<img res="@atlas/ui/cancel.png" tint="0x333333" pivot="50%,50%" />
				<circle radius="29" color="0xF3DE3F" />
			</layer>
			<layer id="disable">
				<img res="@atlas/ui/cancel.png" tint="0x333333" pivot="50%,50%" />
				<circle radius="29" color="0x333333" />
			</layer>
		</buttonSlide>
		<rectangle width="440" height="152" alpha="0.65" color="0x808080" bcolor="0x333333" border="2" radius="35" interactive="true" />
	</layer>
