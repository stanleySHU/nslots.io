<layer id="hud">
	<placeholder id="cheatBar">
		<cheatBar id="cheatBar" keys="1:Small Win,2:Big Win,3:Feature" x="0" scale="1"/>
	</placeholder>
	<placeholder id="btnBonusEffect"> 
		<img res="@atlas/sprites/btneffect.png" id="btnBonusEffect" pivot="50%,50%" scale="1.2" x="888" y="269" />
	</placeholder>
	<placeholder id="menu"> 
		<include ref="../../_commons/src/layout/4.1/menu.mobile.both.xml"/>
	</placeholder>
	<placeholder id="autoSpin"> 
		<include ref="../../_commons/src/layout/4.1/autospin.landscape.xml" fontSize="22"/>
	</placeholder>
	<placeholder id="betPanel"> 
		<include ref="../../_commons/src/layout/4.1/betPanel.landscape.xml"/>
	</placeholder>
	<placeholder id="tournament"> 
		<tournament id="tournament" x="52" y="344" visible="false">
			<buttonSlide id="btTournament"   command="tournament"> 
				<movieClip id="up" res="@sheet/tournamenticon" loop="-1" speed="0.1" pivot="50%,50%" />
				<movieClip id="over" res="@sheet/tournamenticon" loop="-1" speed="0.1" pivot="50%,50%" />
				<movieClip id="disable" res="@sheet/tournamenticon" loop="1" play="false" speed="0.1" tint="0x888888" pivot="50%,50%" />
			</buttonSlide>
		</tournament>
	</placeholder>
	<if name="fround" value="1">
		<placeholder id="fround">
			<include ref="../../_commons/src/layout/4.1/froundToggle.landscape.xml"/>
		</placeholder>
	</if>
	<placeholder id="btnMenu">
		<button id="btnMenu" x="12" y="445" up="@atlas/ui/menu-on.png" disabled="@atlas/ui/menu.png" onClick="clickMenuHandler" />
	</placeholder>
	<placeholder id="autoSpinCounterDisplay">
		<layer id="autoSpinDisplay" x="890" y="160" >
			<img id="btnEffectStopAuto"  res="@atlas/ui/stopautoeffect.png" scale="1,1" pivot="50%,50%" tint="0xf2cf75"/>
			<label id="autoSpinCount"  align="center" fontFamily="Din, Arial" fontSize="24" fill="0x333333" pivot="0,50%" />
			<img id="autoSpinInfinity"  visible="false" res="@atlas/ui/icon-infinity.png" scale="0.75,0.75" tint="0x333333" pivot="50%,50%" />
			<button id="btnStopAuto"  onClick="stopAutoHandler" scale="1,1" pivot="50%,50%" tint="0xf2cf75">
				<img id="up" res="@atlas/ui/autspincount.png" tint="0xf2cf75"  />
			</button>		
		</layer>	
	</placeholder>
	<placeholder id="btnFundReal"> 
		<button id="realPlay" up="@atlas/ui/menu-playforreal-on.png" disabled="@atlas/ui/menu-playforreal-off.png" onClick="clickRealPlayHandler" x="905" y="445" />
		<button id="fund" up="@atlas/ui/menu-fundtransfer-on.png" disabled="@atlas/ui/menu-fundtransfer-off.png" onClick="clickFundHandler" x="905" y="445" />	
	</placeholder>
	<placeholder id="betButtons">
		<spinButton id="btnSpin" scale="1" x="890" y="270" pivot="50%,50%" circleHit="true" spin="@atlas/sprites/spin.png" skip="@atlas/sprites/skip.png" spinHint="@atlas/sprites/spinhint.png" stop="@atlas/sprites/stop.png" bonus="@atlas/sprites/play.png" command="spin" />
		<button id="btnAutoSpin" x="890" y="102" onClick="clickAutospin" >
			<img id="up" y="32" pivot="50%,50%" res="@atlas/ui/icon-autospin.png" tint="0xf2cf75" />
			<img id="text-l" y="73" pivot="50%,50%" res="@atlas/ui/autospintext-l.png" tint="0xf2cf75"/>
			<img id="text-p" visible="false" y="55" pivot="50%,50%" res="@atlas/ui/autospintext-p.png" tint="0xf2cf75"/>
		</button>
		<button x="890" y="338" id="btnBetSettings" onClick="clickSettings">
			<img id="up" y="35" pivot="50%,50%" res="@atlas/ui/icon-bet.png" tint="0xf2cf75" />
			<img id="text" res="@atlas/ui/bettext.png" y="72" pivot="50%,50%" tint="0xf2cf75"/>
		</button>
		<img res="@atlas/sprites/betpanel-bg.png" x="812" y="75"  scale="1" />
	</placeholder>
	<rectangle width="960" height="540" alpha="0" />
</layer>
