					<layer id="up">
						<label value="{%n%}" align="center" fontFamily="Din, Arial" fontSize="30" fill="0xFFFFFF" pivot="0,50%"/>
						<circle radius="29" color="0x3D3D4B" border="1" innerShadow="0x5C5C72" dropShadow="0x000000"  />
					</layer>
					<layer id="down">
						<label value="{%n%}" align="center" fontFamily="Din, Arial" fontSize="30" fill="0xFFFFFF" pivot="0,50%"/>
						<circle radius="29" color="{{colorTheme}}" border="1" innerShadow="{{colorThemeHighlight}}" dropShadow="0x000000"   />
					</layer>
					<layer id="disable">
						<label value="{%n%}" align="center" fontFamily="Din, Arial" fontSize="30" fill="0x333333" pivot="0,50%"/>
						<circle radius="29" color="0x3D3D4B" border="1" innerShadow="0x5C5C72" dropShadow="0x000000"  />
					</layer>
				</buttonSlide>
			</content>			
			<arguments>
