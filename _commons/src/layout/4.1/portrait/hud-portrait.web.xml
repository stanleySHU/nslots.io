<layer id="hud">
	<placeholder id="cheatBar">
		<cheatBar id="cheatBar" keys="1:Small Win,2:Big Win,3:Feature" />
	</placeholder>	
	<placeholder id="infoPanel">
		<include ref="../../_commons/src/layout/4.1/infoPanel.web.xml"/>
	</placeholder>
	<layer id="boxTotalBet" x="417" y="850" >
		<animatedLabel id="txtTotalBet" value="0.00" align="center" fontFamily="Din, Arial" fontSize="20" pivot="0,50%" />
		<img res="@atlas/ui/total-bet.png" pivot="50%,50%" y="2" />
	</layer>
	<placeholder id="btnBonusEffect" />	
	<include ref="../../_commons/src/layout/4.1/portrait/autospin-portrait.web.xml"/>
	<spinButton id="btnSpin" x="270" y="815">
		<button id="spin" command="spin">
			<circle id="hit" radius="60" pivot="50%,50%" />
			<img id="up" res="@atlas/sprites/spin.png" pivot="50%,50%" />
			<img id="over" res="@atlas/sprites/spin.png" pivot="50%,50%" />
			<img id="disable" res="@atlas/sprites/spin.png" pivot="50%,50%" tint="0x888888" />
		</button>
		<button id="skip" command="spin">
			<circle id="hit" radius="60" pivot="50%,50%" />
			<img id="up" res="@atlas/sprites/spin_skip.png" pivot="50%,50%" />
			<img id="over" res="@atlas/sprites/spin_skip.png" pivot="50%,50%" />
			<img id="disable" res="@atlas/sprites/spin_skip.png" pivot="50%,50%" tint="0x888888" />
		</button>
		<button id="stop" command="spin">
			<circle id="hit" radius="60" pivot="50%,50%" />
			<img id="up" res="@atlas/sprites/spin_stop.png" pivot="50%,50%" />
			<img id="over" res="@atlas/sprites/spin_stop.png" pivot="50%,50%" />
			<img id="disable" res="@atlas/sprites/spin_stop.png" pivot="50%,50%" tint="0x888888" />
		</button>
		<button id="bonus" command="spin">
			<circle id="hit" radius="60" pivot="50%,50%" />
			<img id="up" res="@atlas/sprites/spin_enabled.png" pivot="50%,50%" />
			<img id="over" res="@atlas/sprites/spin_enabled.png" pivot="50%,50%" />
			<img id="disable" res="@atlas/sprites/spin_enabled.png" pivot="50%,50%" tint="0x888888" />
		</button>
	</spinButton>
	<button id="stopauto" command="stopauto" x="380" y="807">
		<layer id="up">
			<img res="@atlas/ui/text_stop_auto.png" pivot="50%,50%" scale="0.7"/>
			<img  res="@atlas/sprites/button_enabled.png" pivot="50%,50%"/>
		</layer>
		<layer id="over">
			<img res="@atlas/ui/text_stop_auto.png" pivot="50%,50%" scale="0.7"/>
			<img res="@atlas/sprites/button_enabled.png" pivot="50%,50%"/>
		</layer>
		<layer id="disable">
			<img res="@atlas/ui/text_stop_auto.png" pivot="50%,50%" tint="0x888888" scale="0.7"/>
			<img res="@atlas/sprites/button_enabled.png" pivot="50%,50%" tint="0x888888"/>
		</layer>
	</button>
	<layer id="autoSpinDisplay" x="400" y="810">
		<label id="autoSpinCount" x="40" align="center" y="-13" fontFamily="Din, Arial" fontSize="20" fill="0xFFFFFF" />
		<img id="autoSpinInfinity" x="40" y="-3" visible="false" res="@atlas/ui/autospin_infinity.png" pivot="50%,40%" />
		<img res="@atlas/sprites/panel-small.png" pivot="50%,50%" />
	</layer>
	<comboBox id="betSelection" x="50" y="833" top="@atlas/ui/total-bet.png"
		dropdown-x="28" numItemsShown="5" itemsTextX="-14"
		itemOffsetY="4"
		dropdown="@atlas/ui/linebet_list.png" highlight="@atlas/sprites/popup-select.png" dropdownPosition="top" 
		itemsTextColor="0xFFFFFF" itemGap="3" itemPadding="8" mouseWheelGap="27" scrollPadding="18"
		scrollbutton="@atlas/sprites/scrollerbar_in.png"
      	scrollbar="@atlas/sprites/scrollerbar_out.png" 
		scrollbarY="-9"
		align="center" fontFamily="Din, Arial" fontSize="20" selectedItemTextColor="0xFFFFFF" 
		arrowSize="14" arrowColor="0xE4E4E4" arrowPadding="10" 
	/>
	<placeholder id="maxBetAnim">
	<slide id="maxBetAnim" x="160" y="807" visible="false">
		<movieClip id="enabled" res="@sheet/maxbetEffect" loop="-1" activate="true" pivot="50%,50%" speed="0.3" scale="1" />
		<movieClip id="disabled" res="@sheet/maxbetEffect" loop="-1" activate="true" pivot="50%,50%" speed="0.3" scale="1" tint="0x999999" />
	</slide>
	</placeholder>
	<slide id="maxbetSelected" x="160" y="807" visible="false">	
		<layer id="enabled">
			<img res="@atlas/ui/text_max_bet.png" pivot="50%,50%" scale="0.7"/>
			<img res="@atlas/sprites/button_enabled.png" pivot="50%,50%"/>
		</layer>	
		<layer id="disabled">
			<img res="@atlas/ui/text_max_bet.png" pivot="50%,50%" tint="0x888888" scale="0.7"/>
			<img id="disable" res="@atlas/sprites/button_enabled.png" pivot="50%,50%" tint="0x888888"/>
		</layer>		
	</slide>
	<button id="maxbet" command="maxbet" x="160" y="807">
		<layer id="up">
			<img res="@atlas/ui/text_max_bet.png" pivot="50%,50%" scale="0.7"/>
			<img res="@atlas/sprites/button_enabled.png" pivot="50%,50%"/>
		</layer>
		<layer id="over">
			<img res="@atlas/ui/text_max_bet.png" pivot="50%,50%" scale="0.7"/>
			<img res="@atlas/sprites/button_enabled.png" pivot="50%,50%"/>
		</layer>
		<layer id="disable">
			<img res="@atlas/ui/text_max_bet.png" pivot="50%,50%" tint="0x888888" scale="0.7"/>
			<img res="@atlas/sprites/button_enabled.png" pivot="50%,50%" tint="0x888888"/>
		</layer>
	</button>
	<button id="autospin" command="autospin" x="380" y="807">
		<layer id="up">
			<img res="@atlas/ui/text_auto_spin.png" pivot="50%,50%" scale="0.7"/>
			<img res="@atlas/sprites/button_enabled.png" pivot="50%,50%"/>
		</layer>
		<layer id="over">
			<img res="@atlas/ui/text_auto_spin.png" pivot="50%,50%" scale="0.7"/>
			<img res="@atlas/sprites/button_enabled.png" pivot="50%,50%"/>
		</layer>
		<layer id="disable">
			<img res="@atlas/ui/text_auto_spin.png" pivot="50%,50%" tint="0x888888" scale="0.7"/>
			<img res="@atlas/sprites/button_enabled.png" pivot="50%,50%" tint="0x888888"/>
		</layer>
	</button>
	<tournament visible="false" pivot="50%,50%" x="495" y="800"  scale="1.1">
		<button id="btTournament"  command="tournament" > 
			<movieClip id="up" res="@sheet/tournamenticon" loop="-1" speed="0.1"/>
			<movieClip id="over" res="@sheet/tournamenticon" loop="-1" speed="0.1"/>
		</button>
	</tournament>

	<button id="info" command="info" x="30" y="885">
		<img id="up" res="@atlas/ui/info-on.png" pivot="50%,50%" />
		<img id="over" res="@atlas/ui/info-on.png" pivot="50%,50%"/>
		<img id="disable" res="@atlas/ui/info-on.png" pivot="50%,50%" alpha="0.5" />
	</button>
	<menu id="menu" x="0" y="0">		
	<!-- 885 -->
		<button id="toggle" x="492" y="863" command="toggle" pivot="50%,50%"> 
			<img id="up" res="@atlas/ui/menu-on.png" />
			<img id="over" res="@atlas/ui/menu-on.png" />
			<img id="disable" res="@atlas/ui/menu-on.png" alpha="0.5" />
		</button>
		<toggleButton id="sound" x="492" y="816" onClick="clickSoundHandler">
			<button id="up" pivot="50%,50%">
				<img id="up" res="@atlas/ui/menu-soundOn-on.png"/>
				<img id="over" res="@atlas/ui/menu-soundOn-on.png"/>
			</button>
			<button id="down" pivot="50%,50%">
				<img id="up" res="@atlas/ui/menu-soundOff-on.png"/>
				<img id="over" res="@atlas/ui/menu-soundOff-on.png" />
			</button>
		</toggleButton>
		<button id="history" x="492" y="776" command="history" pivot="50%,50%" >
			<img id="up" res="@atlas/ui/menu-history-on.png" />
			<img id="over" res="@atlas/ui/menu-history-on.png"/>
		</button>
		<button id="realPlay" x="492" y="776" command="realPlay" pivot="50%,50%" >
			<img id="up" res="@atlas/ui/menu-playforreal-on.png"/>
			<img id="over" res="@atlas/ui/menu-playforreal-on.png"/>
		</button>
		<button id="fund" x="492" y="776" command="fund" pivot="50%,50%" >
			<img id="up" res="@atlas/ui/menu-fundtransfer-on.png"/>
			<img id="over" res="@atlas/ui/menu-fundtransfer-on.png"/>
		</button>
		<button id="livechat" x="492" y="776" command="livechat" pivot="50%,50%" >
			<img id="up" res="@atlas/ui/menu-livechat-on.png" />
			<img id="over" res="@atlas/ui/menu-livechat-on.png"/>
		</button>
		<if name="help" value="1">
			<button id="help" x="492" y="866" command="help" pivot="50%,50%" >
				<img id="up" res="@atlas/ui/menu-questionmark-on.png" />
				<img id="over" res="@atlas/ui/menu-questionmark-on.png"/>
			</button>
		</if>
		<button id="lobby" x="492" y="771" command="lobby" pivot="50%,50%" >
			<img id="up" res="@atlas/ui/menu-back-on.png" />
			<img id="over" res="@atlas/ui/menu-back-on.png"/>
		</button>
		<button id="home" x="492" y="729" command="home" pivot="50%,50%" > 
			<img id="up" res="@atlas/ui/menu-home-on.png" />
			<img id="over" res="@atlas/ui/menu-home-on.png"/>
		</button>

		<if name="fround" value="1">
			<froundToggle id="fround" x="190" y="490" onClick="clickFroundHandler" visible="false">
				<layer x="70">
					<label y="-8" x="-1" bitmap="true" id="froundCount" align="center" fontFamily="number" fontSize="20" fill="0xffffff"/>
					<circle radius="14" color="0xff0000"/>
					<circle radius="16" color="0xffffff"/>
				</layer>
				<layer id="up">
					<layer>
						<img res="@atlas/fround/box-close.png"/>
						<img id="shine" res="@atlas/fround/shine.png" pivot="70px,65px" x="35" y="28"/>
					</layer>
					<tween name="box-anim" auto="true"/>
				</layer>
				<layer id="down">
					<img res="@atlas/fround/box-open.png"/>
					<img res="@atlas/fround/freerounds-{{LANGUAGE}}.png" pivot="50%,50%" y="70" scale="0.8" x="45"/>
					<img id="shine2" res="@atlas/fround/shine.png" pivot="70px,65px" x="45" y="35"/>
				</layer>
			</froundToggle>
		</if>
	</menu>	
	<placeholder id="buttons" />
	<placeholder id="hudLabels">
		<img res="@atlas/ui/linebet_text.png" x="126" y="872" pivot="50%,0%" scale="0.9" />
		<img res="@atlas/ui/totalbet.png" x="418" y="872" pivot="50%,0%" scale="0.9" />
	</placeholder>
	<rectangle id="clickable" width="1280" height="738" alpha="0" />
</layer>