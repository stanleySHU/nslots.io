<layer id="hud">
	<placeholder id="cheatBar">
		<cheatBar id="cheatBar" keys="1:Small Win,2:Big Win,3:Feature" />
	</placeholder>	
	<placeholder id="infoPanel">
		<infoPanel id="infoPanel" pages="{{numInfoPages}}" visible="false">
			<button id="infoBack" x="534" y="660" command="infoBack">
				<img id="up" res="@atlas/ui/button-BACK.png"/>
				<img id="over" res="@atlas/ui/button-BACK-on.png" />
			</button>
			<button id="infoPrev" x="487" y="660" command="infoPrev">
				<img id="up" res="@atlas/ui/info-arrow.png"/>
				<img id="over" res="@atlas/ui/info-arrow-on.png" />
			</button>
			<button id="infoNext" x="795" y="700" command="infoNext">
				<img id="up" res="@atlas/ui/info-arrow.png" rotation="180" />
				<img id="over" res="@atlas/ui/info-arrow-on.png" rotation="180" />
			</button>
			<img res="@img/bg-info" interactive="true"/>
		</infoPanel>	
	</placeholder>
	<layer id="boxTotalBet" x="290" y="640" >
		<animatedLabel id="totalBet" value="0.00" align="center" fontFamily="Din, Arial" fontSize="20" pivot="0,50%" />
		<img res="@atlas/ui/total-bet.png" pivot="50%,50%" />
	</layer>
	<comboBox id="betSelection" x="364" y="620" top="@atlas/ui/line-bet.png" 
		topOver="@atlas/ui/arrow-on.png" topOverX="98" topOverY="4" numItemsShown="5" 
		dropdown="@atlas/ui/line-bet-popup.png" highlight="@atlas/ui/popup-select.png" dropdownPosition="top" 
		itemsTextColor="0xFFFFFF" itemGap="2" itemPadding="7" mouseWheelGap="27"
		barColor="0xFC4B49" barWidth="8" barHeight="107" barRadius="4" barTransparency="0.3"
      	buttonColor="0xFC4B49" buttonWidth="6" buttonHeight="20" buttonRadius="3"  
		align="center" fontFamily="Din, Arial" fontSize="20"  />
	<spinButton id="btnSpin" x="682" y="640">
		<button id="spin" command="spin">
			<img id="up" res="@atlas/ui/button-SPIN.png" pivot="50%,50%" />
			<img id="over" res="@atlas/ui/button-SPIN.png" pivot="50%,50%" />
			<img id="disable" res="@atlas/ui/button-SPIN-disabled.png" pivot="50%,50%" />
		</button>
		<button id="skip" command="spin">
			<img id="up" res="@atlas/ui/button-SKIP.png" pivot="50%,50%" />
			<img id="over" res="@atlas/ui/button-SKIP.png" pivot="50%,50%" />
			<img id="disable" res="@atlas/ui/button-SKIP-disabled.png" pivot="50%,50%" />
		</button>
		<button id="stop" command="spin">
			<img id="up" res="@atlas/ui/button-STOP.png" pivot="50%,50%" />
			<img id="over" res="@atlas/ui/button-STOP.png" pivot="50%,50%" />
			<img id="disable" res="@atlas/ui/button-SKIP-disabled.png" pivot="50%,50%" />
		</button>
		<button id="bonus" command="spin">
			<img id="up" res="@atlas/ui/button-PLAY.png" pivot="50%,50%" />
			<img id="over" res="@atlas/ui/button-PLAY.png" pivot="50%,50%" />
			<img id="disable" res="@atlas/ui/button-PLAY-disabled.png" pivot="50%,50%" />
		</button>
	</spinButton>
	<placeholder id="btnSpinBg">
		<img id="btnSpinBg" res="@atlas/sprites/button-spin-bg.png" x="682" y="640" pivot="50%,50%" />
	</placeholder>
	<slide id="maxbetSelected" x="565" y="640" visible="false">		
		<img id="enabled" res="@atlas/ui/button-MAXBET-On.png" pivot="50%,50%"/>
		<img id="disabled" res="@atlas/ui/button-MAXBET-On-disabled.png" pivot="50%,50%" />
	</slide>
	<button id="maxbet" command="maxbet" x="565" y="640">
		<img id="up" res="@atlas/ui/button-MAXBET.png" pivot="50%,50%"/>
		<img id="over" res="@atlas/ui/button-MAXBET.png" pivot="50%,50%"/>
		<img id="disable" res="@atlas/ui/button-MAXBET-disabled.png" pivot="50%,50%"/>
	</button>
	<button id="autospin" command="autospin" x="800" y="640">
		<img id="up" res="@atlas/ui/button-AUTOSPIN.png" pivot="50%,50%"/>
		<img id="over" res="@atlas/ui/button-AUTOSPIN.png" pivot="50%,50%"/>
		<img id="disable" res="@atlas/ui/button-AUTOSPIN-disabled.png" pivot="50%,50%"/>
	</button>
	<button id="stopauto" command="stopauto" x="800" y="640">
		<img id="up" res="@atlas/ui/button-STOPAUTO-On.png" pivot="50%,50%"/>
		<img id="over" res="@atlas/ui/button-STOPAUTO-On.png" pivot="50%,50%"/>
		<img id="disable" res="@atlas/ui/button-STOPAUTO-On-disabled.png" pivot="50%,50%"/>
	</button>
	<comboBox id="autoSelection" x="868" y="620" top="@atlas/ui/auto-spin.png" 
		topOver="@atlas/ui/arrow-on.png" topOverX="64.5" topOverY="4" numItemsShown="5" 
		dropdown="@atlas/ui/auto-spin-popup.png" highlight="@atlas/ui/auto-spin-popup-select.png" dropdownPosition="top" 
		itemsTextColor="0xFFFFFF" itemGap="2" align="center" fontFamily="Din, Arial" fontSize="20" />
	<button id="info" command="info" x="998" y="640">
		<img id="up" res="@atlas/ui/info.png" pivot="50%,50%" />
		<img id="over" res="@atlas/ui/info-on.png" pivot="50%,50%" />
		<img id="disable" res="@atlas/ui/info.png" pivot="50%,50%" alpha="0.5" />
	</button>
	<menu id="menu" x="0" y="0">		
		<button id="toggle" x="1026" y="618" command="toggle" pivot="50%,50%">
			<img id="up" res="@atlas/ui/menu.png" />
			<img id="over" res="@atlas/ui/menu-on.png" />
			<img id="disable" res="@atlas/ui/menu.png" alpha="0.5" />
		</button>
		<toggleButton id="sound" x="1026" y="573" onClick="clickSoundHandler">
			<button id="up" pivot="50%,50%">
				<img id="up" res="@atlas/ui/menu-soundOn-off.png" />
				<img id="over" res="@atlas/ui/menu-soundOn-on.png" />
			</button>
			<button id="down" pivot="50%,50%">
				<img id="up" res="@atlas/ui/menu-soundOff-off.png" />
				<img id="over" res="@atlas/ui/menu-soundOff-on.png" />
			</button>
		</toggleButton>
		<button id="history" x="1026" y="528" command="history" pivot="50%,50%" >
			<img id="up" res="@atlas/ui/menu-history-off.png" />
			<img id="over" res="@atlas/ui/menu-history-on.png" />
		</button>
		<button id="realPlay" x="1026" y="528" command="realPlay" pivot="50%,50%" >
			<img id="up" res="@atlas/ui/menu-playforreal-off.png" />
			<img id="over" res="@atlas/ui/menu-playforreal-on.png" />
		</button>
		<button id="fund" x="1026" y="528" command="fund" pivot="50%,50%" >
			<img id="up" res="@atlas/ui/menu-fundtransfer-off.png" />
			<img id="over" res="@atlas/ui/menu-fundtransfer-on.png" />
		</button>
		<button id="livechat" x="1026" y="528" command="livechat" pivot="50%,50%" >
			<img id="up" res="@atlas/ui/menu-livechat-off.png" />
			<img id="over" res="@atlas/ui/menu-livechat-on.png" />
		</button>
		<if name="help" value="1">
			<button id="help" x="1026" y="528" command="help" pivot="50%,50%" >
				<img id="up" res="@atlas/ui/menu-questionmark-off.png" />
				<img id="over" res="@atlas/ui/menu-questionmark-on.png" />
			</button>
		</if>
		<button id="lobby" x="1026" y="528" command="lobby" pivot="50%,50%" >
			<img id="up" res="@atlas/ui/menu-back-off.png" />
			<img id="over" res="@atlas/ui/menu-back-on.png" />
		</button>
		<button id="home" x="1026" y="528" command="home" pivot="50%,50%" > 
			<img id="up" res="@atlas/ui/menu-home-off.png" />
			<img id="over" res="@atlas/ui/menu-home-on.png" />
		</button>	
		<if name="fround" value="1">
			<froundToggle id="fround" x="190" y="520" onClick="clickFroundHandler" visible="false">
				<layer x="70">
					<label y="-8" x="-1" bitmap="true" id="froundCount" align="center" fontFamily="number" fontSize="20" fill="0xffffff"/>
					<circle radius="14" color="0xff0000"/>
					<circle radius="16" color="0xffffff"/>
				</layer>
				<layer id="up">
					<layer>
						<img res="@atlas/fround/box-close.png"/>
						<img id="shine" res="@atlas/fround/shine.png" pivot="70px,65px" x="35" y="28"/>
					</layer>
					<tween name="box-anim" auto="true"/>
				</layer>
				<layer id="down">
					<img res="@atlas/fround/box-open.png"/>
					<img res="@atlas/fround/freerounds-{{LANGUAGE}}.png" pivot="50%,50%" y="70" scale="0.8" x="45"/>
					<img id="shine2" res="@atlas/fround/shine.png" pivot="70px,65px" x="45" y="35"/>
				</layer>
			</froundToggle>
		</if>
	</menu>
	<placeholder id="hudLabels">
		<hudLabel bitmap="true" x="289" y="665" value="{{totalbet}}" default="TOTAL BET" align="center" case="upper" fontFamily="hudfont" fontSize="21" fill="0xFFFFFF" stroke="0x272727" strokeThickness="4" />
		<hudLabel bitmap="true" x="433" y="665" value="{{linebet}}" default="LINE BET" align="center" case="upper" fontFamily="hudfont" fontSize="21" fill="0xFFFFFF" stroke="0x272727" strokeThickness="4" />
		<hudLabel bitmap="true" x="917" y="665" value="{{autospin}}" default="AUTO SPIN" align="center" case="upper" fontFamily="hudfont" fontSize="21" fill="0xFFFFFF" stroke="0x272727" strokeThickness="4" />
	</placeholder>
	<rectangle id="clickable" width="1280" height="768" alpha="0" />
</layer>