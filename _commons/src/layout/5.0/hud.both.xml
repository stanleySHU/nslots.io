<layer id="hud">
	<placeholder id="menu"> 
		<include ref="../../_commons/src/layout/5.0/menu.both.xml"/>
	</placeholder>
	<placeholder id="autoSpin"> 
		<include ref="../../_commons/src/layout/5.0/autoSpin.both.xml" />
	</placeholder>
	<layer id="selectionPanel" y="100">
		<placeholder id="betSelection">
			<betSelection id="betSelection" l-x="860" l-y="570" p-x="548" p-y="991.5" pivot="50%,100%" p-scale="0.9" l-scale="1" >
				<buttonSlide id="btnClose">
						<layer id="up"> 
								<rectangle x="60" y="199" width="65" height="3" color="0xFFBC30" />
						</layer>
						<layer id="disable"> 
								<rectangle x="60" y="199" width="65" height="3" color="0x87611B" />
						</layer>
				</buttonSlide>
				<carousel id="selection" x="0" y="84" 
						numItemsShown="5" dropdownWidth="180" itemHeight="35"
						selectItemOnClick="true" mouseWheelEnabled="true" mouseWheelGap="35" 
						itemsTextColor="0xBFBFBF" highlightColor="0x3D3D4B" 
						scaleTopBottomItems="2" scaleTopBottomValue="0.12"
						align="center" fontFamily="Din, Arial" fontWeight="bold" 
						fontSize="24" selectedItemTextColor="0xFFB800" />
				<rectangle y="85" x="2" width="181" height="30" color="0x130731" alpha="0.9" />
				<rectangle width="185" height="200" color="0x1e0c4a" border="3" bcolor="0x888888" alpha="0.8" radius="10" interactive="true" />
			</betSelection>
		</placeholder>
		<rectangle id="clickable" x="-100" y="-100" width="1270" height="1270" alpha="0" interactive="false" />	
		<placeholder id="btnBetSelection">
			<layer id="iconBetExpandable" l-x="858.5" l-y="580" p-x="548.5" p-y="1001" pivot="50%,50%" interactive="false">				
				<layer l-scale="1" p-scale="0.9">
					<polygon path="0,8,20,0,40,8,33,8,20,3,7,8" color="0xCCCCCC" />
				</layer>
			</layer>
			<buttonSlide id="btnBetSelection" command="betSelection" l-x="766" l-y="571" p-x="463" p-y="992" p-scale="0.9" l-scale="1">
				<layer id="up"> 
					<rectangle x="60" width="65" height="3" color="0xFFBC30" />
					<rectangle y="2" width="185" height="60" alpha="0" radius="10" />
				</layer>
				<layer id="disable">
					<rectangle x="60" width="65" height="3" color="0x87611B" />
					<rectangle y="2" width="185" height="60" alpha="0" radius="10" />
				</layer>
			</buttonSlide>
		</placeholder>
	</layer>
	<placeholder id="cheatBar">
		<cheatBar id="cheatBar" keys="1:Small Win,2:Big Win,3:Feature" l-x="0" p-x="-70" p-scale="0.69,0.8" l-scale="1"/>
	</placeholder>	
	<placeholder id="tournament"> 
		<tournament id="tournament" l-x="165" l-y="435" p-x="585" p-y="835" visible="false">
			<buttonSlide id="btTournament" command="tournament" > 
				<movieClip id="up" res="@sheet/tournamenticon" loop="-1" speed="0.1" pivot="50%,50%" />
				<movieClip id="over" res="@sheet/tournamenticon" loop="-1" speed="0.1" pivot="50%,50%" />
				<movieClip id="disable" res="@sheet/tournamenticon" loop="1" play="false" speed="0.1" tint="0x888888" pivot="50%,50%" />
			</buttonSlide>
		</tournament>
	</placeholder>
	<if name="fround" value="1">
		<placeholder id="fround">
			<include ref="../../_commons/src/layout/5.0/froundToggle.both.xml"/>
		</placeholder>
	</if>
	<layer id="betButtons" l-y="0" p-y="50">
		<placeholder id="btnBonusEffect" />
		<placeholder id="btnSpinEffect"> 
			<movieClip id="btnSpinEffect" res="@sheet/btnspineffect" l-x="980" l-y="332" p-x="370" p-y="932" pivot="50%,50%" loop="1" activate="false" play="false" speed="0.3" blendMode="screen" tint="{{btnSpinIconTint}}" visible="false" interactive="false" />
		</placeholder>
		<placeholder id="btnSpin">
			<spinButton id="btnSpin" l-x="980" l-y="330" p-x="370" p-y="930">
				<buttonSlide id="spin" command="spin" sound=" ">
					<img id="up" res="@atlas/ui/btn-spin-base.png" pivot="50%,50%" />
					<img id="disable" res="@atlas/ui/btn-spin-base.png" pivot="50%,50%" tint="0x888888" />
					<circle id="hit" radius="55" pivot="50%,50%" alpha="0" />
				</buttonSlide>
				<buttonSlide id="skip" command="spin" sound=" ">
					<layer id="up"> 
						<img res="@atlas/ui/btn-skip.png" pivot="50%,50%" tint="{{btnSpinIconTint}}" />
						<img res="@atlas/ui/btn-spin-base.png" pivot="50%,50%" />
					</layer>
					<layer id="disable"> 
						<img res="@atlas/ui/btn-skip.png" pivot="50%,50%" tint="{{btnSpinIconDisabledTint}}" />
						<img res="@atlas/ui/btn-spin-base.png" pivot="50%,50%" tint="0x888888" />
					</layer>
					<circle id="hit" radius="55" pivot="50%,50%" alpha="0" />
				</buttonSlide>
				<buttonSlide id="stop" command="spin">
					<layer id="up"> 
						<img res="@atlas/ui/btn-stop.png" pivot="50%,50%" tint="{{btnSpinIconTint}}" />
						<img res="@atlas/ui/btn-spin-base.png" pivot="50%,50%" />
					</layer>
					<layer id="disable"> 
						<img res="@atlas/ui/btn-stop.png" pivot="50%,50%" tint="{{btnSpinIconDisabledTint}}" />
						<img res="@atlas/ui/btn-spin-base.png" pivot="50%,50%" tint="0x888888" />
					</layer>
					<circle id="hit" radius="55" pivot="50%,50%" alpha="0" />
				</buttonSlide>
				<buttonSlide id="bonus" command="spin">
					<layer id="up"> 
						<img res="@atlas/ui/btn-play.png" pivot="50%,50%" tint="{{btnSpinIconTint}}" />
						<img res="@atlas/ui/btn-spin-base.png" pivot="50%,50%" />
					</layer>
					<layer id="disable"> 
						<img res="@atlas/ui/btn-play.png" pivot="50%,50%" tint="{{btnSpinIconDisabledTint}}" />
						<img res="@atlas/ui/btn-spin-base.png" pivot="50%,50%" tint="0x888888" />
					</layer>
					<circle id="hit" radius="55" pivot="50%,50%" alpha="0" />
				</buttonSlide>
				<img res="@atlas/ui/btn-spin.png" id="spinHint" pivot="50%,50%" tint="{{btnSpinIconTint}}" spinHintDisabledTint="{{btnSpinIconDisabledTint}}" interative="false" />	
			</spinButton>		
		</placeholder>
		<placeholder id="btnAutoSpin">
			<layer id="autoSpinDisplay" l-x="980" l-y="215" p-x="242" p-y="930">
				<label id="autoSpinCount" align="center" fontFamily="Din, Arial" fontSize="25" fontWeight="bold" fill="0x333333" pivot="0,50%" />
				<img id="autoSpinInfinity" visible="false" res="@atlas/ui/icon-infinity.png" tint="0x333333" scale="0.8" pivot="50%,50%" />
				<buttonSlide id="btnStopAuto" command="stopauto">
					<layer id="up">
						<layer>
							<img res="@atlas/ui/btn-stop-auto.png" pivot="50%,50%" blendMode="add" />
							<img res="@atlas/ui/btn-auto-spin.png" pivot="50%,50%" tint="{{btnSpinIconTint}}" blendMode="screen" />
							<img res="@atlas/ui/btn-auto-turbo-glow.png" pivot="50%,50%" tint="{{btnSpinIconTint}}" blendMode="screen" />
							<tween name="btn-highlight-glow" auto="true" />
						</layer>
						<img res="@atlas/ui/btn-stop-auto.png" pivot="50%,50%" />
						<img res="@atlas/ui/btn-auto-spin.png" pivot="50%,50%" tint="{{btnSpinIconTint}}" />
						<img res="@atlas/ui/btn-auto-turbo-base.png" pivot="50%,50%" />
					</layer>
					<layer id="disable">
						<img res="@atlas/ui/btn-stop-auto.png" pivot="50%,50%" tint="0x888888" />
						<img res="@atlas/ui/btn-auto-spin.png" pivot="50%,50%" tint="{{btnSpinIconDisabledTint}}" />
						<img res="@atlas/ui/btn-auto-turbo-base.png" pivot="50%,50%" tint="0x888888" />
					</layer>
				<circle id="hit" radius="33" pivot="50%,50%" alpha="0" />
				</buttonSlide>
			</layer>
			<buttonSlide id="btnAutoSpin" l-x="980" l-y="215" p-x="242" p-y="930" command="autospin">
				<layer id="up">
					<img res="@atlas/ui/btn-auto-play.png" pivot="50%,50%" tint="{{btnSpinIconTint}}" />
					<img res="@atlas/ui/btn-auto-spin.png" pivot="50%,50%" tint="{{btnSpinIconTint}}" />
					<img res="@atlas/ui/btn-auto-turbo-base.png" pivot="50%,50%" />
				</layer>
				<layer id="disable">
					<img res="@atlas/ui/btn-auto-play.png" pivot="50%,50%" tint="{{btnSpinIconDisabledTint}}" />
					<img res="@atlas/ui/btn-auto-spin.png" pivot="50%,50%" tint="{{btnSpinIconDisabledTint}}" />
					<img res="@atlas/ui/btn-auto-turbo-base.png" pivot="50%,50%" tint="0x888888" />
				</layer>
				<circle id="hit" radius="33" pivot="50%,50%" alpha="0" />
			</buttonSlide>
		</placeholder>
		<placeholder id="btnTurbo">
			<buttonSlide id="btnStopturbo" l-x="980" l-y="445" p-x="498" p-y="930" command="turbo">
				<layer id="up">
					<layer>
							<img res="@atlas/ui/btn-turbo.png" pivot="50%,50%" tint="{{btnSpinIconTint}}" scale="0.75" blendMode="screen" />
							<img res="@atlas/ui/btn-stop-turbo.png" pivot="50%,50%" blendMode="add" />
							<img res="@atlas/ui/btn-auto-turbo-glow.png" pivot="50%,50%" tint="{{btnSpinIconTint}}" blendMode="screen" />
							<tween name="btn-highlight-glow" auto="true" />
					</layer>
					<img res="@atlas/ui/btn-turbo.png" pivot="50%,50%" tint="{{btnSpinIconTint}}" scale="0.75" />
					<img res="@atlas/ui/btn-stop-turbo.png" pivot="50%,50%" />
					<img res="@atlas/ui/btn-auto-turbo-base.png" pivot="50%,50%" />
				</layer>
				<layer id="disable">
					<img res="@atlas/ui/btn-turbo.png" pivot="50%,50%" tint="{{btnSpinIconDisabledTint}}" scale="0.75" />
					<img res="@atlas/ui/btn-stop-turbo.png" pivot="50%,50%" tint="0x888888" />
					<img res="@atlas/ui/btn-auto-turbo-base.png" pivot="50%,50%" tint="0x888888" />
				</layer>
				<circle id="hit" radius="33" pivot="50%,50%" alpha="0" />
			</buttonSlide>
			<buttonSlide id="btnTurbo" l-x="980" l-y="445" p-x="498" p-y="930" command="turbo">
				<layer id="up">
					<img res="@atlas/ui/btn-turbo.png" pivot="50%,50%" tint="{{btnSpinIconTint}}" />
					<img res="@atlas/ui/btn-auto-turbo-base.png" pivot="50%,50%" />
				</layer>
				<layer id="disable">
					<img res="@atlas/ui/btn-turbo.png" pivot="50%,50%" tint="{{btnSpinIconDisabledTint}}" />
					<img res="@atlas/ui/btn-auto-turbo-base.png" pivot="50%,50%" tint="0x888888" />
				</layer>
				<circle id="hit" radius="33" pivot="50%,50%" alpha="0" />
			</buttonSlide>
		</placeholder>
	</layer>
	<layer id="settingButtons" l-y="100" p-y="50">
		<placeholder id="btnMenu">
			<buttonSlide id="btnMenu" l-x="150" l-y="604" p-x="140" p-y="930" command="menu">
				<img id="up" res="@atlas/ui/btn-menu.png" pivot="50%,50%" />
				<img id="disable" res="@atlas/ui/btn-menu.png" pivot="50%,50%" tint="0x888888" />
				<circle id="hit" radius="29" pivot="50%,50%" alpha="0" />
			</buttonSlide>		
		</placeholder>
		<placeholder id="btnRealFund">
			<buttonSlide id="realPlay" command="realplay" l-x="1020" l-y="604" p-x="600" p-y="930">
				<img id="up" res="@atlas/ui/btn-realplay.png" pivot="50%,50%" />
				<img id="disable" res="@atlas/ui/btn-realplay.png" tint="0x888888" pivot="50%,50%" />
				<circle id="hit" radius="29" pivot="50%,50%" alpha="0" />
			</buttonSlide>
			<buttonSlide id="fund" command="fundtransfer" l-x="1020" l-y="604" p-x="600" p-y="930">	
				<img id="up" res="@atlas/ui/btn-fundtransfer.png" pivot="50%,50%" />
				<img id="disable" res="@atlas/ui/btn-fundtransfer.png" tint="0x888888" pivot="50%,50%" />
				<circle id="hit" radius="29" pivot="50%,50%" alpha="0" />
			</buttonSlide>
		</placeholder>
	</layer>
	<rectangle width="1170" height="740" alpha="0" />
</layer>
