<symbolService total="20" size="88,112" anchor="0.0,0.0">
  <for from="0" to="19" step="1">
    <content>
      <symbol id="{%index%}">
        <state name="idle">
          <layer>
            <spineAnimation id="spine" skin="{%skin%}" res="@res/symbolSpine" auto="false" x="47.5" y="70" action="{%idle%}" scale="1"/>
          </layer>
        </state>
        <state name="spin">
          <layer>
            <!-- <spineAnimation id="spine" skin="{%skin%}" res="@res/symbolSpine" auto="false" x="47.5" y="70" action="{%idle%}" scale="1"/> -->
            <img pivot="50, 50%" x="30" y="52" res="@atlas/spritesAtlas/{%index%}.png" scale="0.65"/>
          </layer>
        </state>
        <state name="animate">
          <layer>
            <spineAnimation id="collapse" skin="default" res="@res/symbolSpine" auto="false" x="46" y="75" action="{%win%}" scale="1" loop="1"/>
            <spineAnimation id="spine" skin="{%skin%}" res="@res/symbolSpine" auto="false" x="47.5" y="70" action="{%idle%}" scale="0.9"/>
            <spineAnimation id="fx" skin="default" res="@res/symbolSpine" auto="false" x="46" y="75" action="{%win%}" scale="1"/>
            <spineAnimation id="highlight" skin="default" res="@res/symbolSpine" auto="false" x="46" y="75" action="{%win%}" scale="1"/>
            <spineAnimation id="wild" skin="Wild_EN" res="@res/symbolSpine" auto="false" x="46" y="75" action="{%win%}" scale="1"/>
          </layer>
        </state>
      </symbol>
    </content>
    <arguments>
      <argument id="0" x="0" y="0" color="0xaaffff" transform="TileTurnHorizontal" drop="TileTrigger" win="TileTrigger_highlight" idle="TileTrigger" skin="2Tiao"/>
      <argument id="1" x="0" y="0" color="0xffbbff" transform="TileTurnHorizontal" drop="TileTrigger" win="TileTrigger_highlight" idle="TileTrigger" skin="2Tong"/>
      <argument id="2" x="0" y="0" color="0xffffcc" transform="TileTurnHorizontal" drop="TileTrigger" win="TileTrigger_highlight" idle="TileTrigger" skin="3Tong"/>
      <argument id="3" x="0" y="0" color="0xeeeeee" transform="TileTurnHorizontal" drop="TileTrigger" win="TileTrigger_highlight" idle="TileTrigger" skin="5Tiao"/>
      <argument id="4" x="0" y="0" color="0xddeeee" transform="TileTurnHorizontal" drop="TileTrigger" win="TileTrigger_highlight" idle="TileTrigger" skin="5Tong"/>
      <argument id="5" x="0" y="0" color="0xee11ee" transform="TileTurnHorizontal" drop="TileTrigger" win="TileTrigger_highlight" idle="TileTrigger" skin="Bawan"/>
      <argument id="6" x="0" y="0" color="0xeeee22" transform="TileTurnHorizontal" drop="TileTrigger" win="TileTrigger_highlight" idle="TileTrigger" skin="Window"/>
      <argument id="7" x="0" y="0" color="0x443333" transform="TileTurnHorizontal" drop="TileTrigger" win="TileTrigger_highlight" idle="TileTrigger" skin="Zhong"/>
      <argument id="8" x="0" y="0" color="0x3344ff" transform="TileTurnHorizontal" drop="TileTrigger" win="TileTrigger_highlight" idle="TileTrigger" skin="Fa"/>
      <argument id="9" x="0" y="0" color="0xff5577" transform="TileTurnHorizontal" drop="Scatter_Drop" win="Scatter_Trigger" idle="Scatter_Idle" skin="default"/>
      <argument id="10" x="0" y="0" color="0x000000" transform="TileTurnHorizontal" drop="Wild_Drop" win="Wild_Trigger" idle="Wild_Idle" skin="Wild_EN"/>
      <argument id="11" x="0" y="0" color="0x000000" transform="TileTurnHorizontal" drop="TileTrigger" win="TileTrigger_highlight" idle="TileTrigger" skin="2Tiao_FS"/>
      <argument id="12" x="0" y="0" color="0x000000" transform="TileTurnHorizontal" drop="TileTrigger" win="TileTrigger_highlight" idle="TileTrigger" skin="2Tong_FS"/>
      <argument id="13" x="0" y="0" color="0x000000" transform="TileTurnHorizontal" drop="TileTrigger" win="TileTrigger_highlight" idle="TileTrigger" skin="3Tong_FS"/>
      <argument id="14" x="0" y="0" color="0x000000" transform="TileTurnHorizontal" drop="TileTrigger" win="TileTrigger_highlight" idle="TileTrigger" skin="5Tiao_FS"/>
      <argument id="15" x="0" y="0" color="0x000000" transform="TileTurnHorizontal" drop="TileTrigger" win="TileTrigger_highlight" idle="TileTrigger" skin="5Tong_FS"/>
      <argument id="16" x="0" y="0" color="0x000000" transform="TileTurnHorizontal" drop="TileTrigger" win="TileTrigger_highlight" idle="TileTrigger" skin="Bawan_FS"/>
      <argument id="17" x="0" y="0" color="0x000000" transform="TileTurnHorizontal" drop="TileTrigger" win="TileTrigger_highlight" idle="TileTrigger" skin="Window_FS"/>
      <argument id="18" x="0" y="0" color="0x000000" transform="TileTurnHorizontal" drop="TileTrigger" win="TileTrigger_highlight" idle="TileTrigger" skin="Zhong_FS"/>
      <argument id="19" x="0" y="0" color="0x000000" transform="TileTurnHorizontal" drop="TileTrigger" win="TileTrigger_highlight" idle="TileTrigger" skin="Fa_FS"/>
    </arguments>
  </for>
</symbolService>